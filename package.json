{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier --write './**/**/*.{ts,js,tsx}'",
    "prettier:check": "prettier --check './**/**/*.{ts,js,tsx}'",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --max-warnings 0",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker kill $(docker ps -q)",
    "prisma:migrate:create": "prisma migrate dev --create-only",
    "prisma:migrate:run": "prisma migrate dev",
    "prisma:deploy": "primsa migrate deploy",
    "prisma:reset": "prisma migrate reset",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
  "dependencies": {
    "@prisma/client": "^3.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "netlify-identity-widget": "^1.9.2",
    "next": "^11.0.0",
    "postgres": "^1.0.2",
    "prettier": "^2.4.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript-eslint": "0.0.1-alpha.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.4",
    "@types/react": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "prisma": "^3.3.0",
    "typescript": "^4.4.4"
  }
}
